{
  "name": "vscode-dark-theme",
  "displayName": "vscode-dark-theme",
  "description": "vscode dark theme",
  "version": "0.0.1",
  "publisher": "blackawn",
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blackawn/vscode-dark-theme.git"
  },
  "contributes": {
    "themes": [
      {
        "label": "Slate Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/slate-theme.json"
      },
      {
        "label": "Gray Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/gray-theme.json"
      },
      {
        "label": "Zinc Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/zinc-theme.json"
      },
      {
        "label": "Stone Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/stone-theme.json"
      },
      {
        "label": "Neutral Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/neutral-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "lint": "eslint .",
    "release": "npm run build && npx bumpp --commit --tag --push --all && vsce publish --no-dependencies",
    "package": "vsce package"
  },
  "dependencies": {
    "vsce": "^2.15.0",
    "esno": "^0.16.3",
    "typescript": "^4.9.5",
    "@types/node": "^18.13.0",
    "nodemon": "^2.0.20"
  }
}
